<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="startPane" alignment="CENTER" spacing="6.0">
         <children>
            <Label fx:id="label1" text="Label" textAlignment="CENTER" />
            <TextField fx:id="ipTextField" alignment="CENTER" promptText="Enter IP" text="localhost:9001" />
            <TextField fx:id="nameTextField" alignment="CENTER" promptText="Enter Name" />
            <TextField fx:id="roomNameTextField" alignment="CENTER" promptText="Enter Room Name" />
            <HBox alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#joinServer" prefHeight="25.0" prefWidth="71.0" text="JOIN" />
                  <Button mnemonicParsing="false" onAction="#hostServer" prefHeight="25.0" prefWidth="71.0" text="HOST" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</Pane>
