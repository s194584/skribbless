<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="startPane" layoutX="240.0" layoutY="40.0">
         <children>
            <Button layoutY="148.0" mnemonicParsing="false" onAction="#joinServer" prefHeight="25.0" prefWidth="71.0" text="JOIN" />
            <TextField fx:id="ipTextField" alignment="CENTER" layoutY="38.0" promptText="Enter IP" text="localhost:9001" />
            <Label fx:id="label1" layoutX="57.0" text="Label" textAlignment="CENTER" />
            <TextField fx:id="nameTextField" alignment="CENTER" layoutY="73.0" promptText="Enter Name" />
            <TextField fx:id="roomNameTextField" alignment="CENTER" layoutY="108.0" promptText="Enter Room Name" />
            <Button layoutX="71.0" layoutY="148.0" mnemonicParsing="false" onAction="#hostServer" prefHeight="25.0" prefWidth="71.0" text="HOST" />
         </children>
      </Pane>
   </children>
</AnchorPane>
